<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<componentSet 
xmlns:comps="http://javascribe.sf.net/patterns/data-object" 
xmlns="http://javascribe.sf.net/config">

	<!-- 
	The Company object will be annotated with Jackson so it can be 
	de-serialized from JSON.
	This is required to receive an object from a HTTP web service 
	client.  It is not required to send a JSON object to the web 
	service client.
	-->
	<comps:jsonObject name="Company" />
	
</componentSet>
