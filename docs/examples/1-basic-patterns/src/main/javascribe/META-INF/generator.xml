		<property name="net.sf.javascribe.patterns.domain.impl.SystemSubsystem">com.melkovia.portal.subsystem.impl.SystemSubsystemImpl</property>
		<property name="net.sf.javascribe.patterns.domain.impl.UserSubsystem">com.melkovia.portal.subsystem.impl.UserSubsystemImpl</property>

		<property name="javascript.min">true</property>
		<property name="net.sf.javascribe.langsupport.javascript.file">www/js/generated.js</property>

